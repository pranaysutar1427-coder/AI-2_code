    "test:csr-regression": "JEST_JUNIT_OUTPUT_NAME=\"csr-regression.xml\" JEST_HTML_OUTPUT_NAME=\"jest_html_report.html\" jest src/csrRegressionTests.spec.js",
    "test:iws1-regression": "JEST_JUNIT_OUTPUT_NAME=\"iws1-regression.xml\" JEST_HTML_OUTPUT_NAME=\"jest_html_report.html\" jest src/agentStation-regression.spec.js",
    "test:qm-regression": "JEST_JUNIT_OUTPUT_NAME=\"qm-regression.xml\" JEST_HTML_OUTPUT_NAME=\"jest_html_report.html\" jest src/qm-regression.spec.js",
    "test:lo-regression": "JEST_JUNIT_OUTPUT_NAME=\"lo-regression.xml\" JEST_HTML_OUTPUT_NAME=\"jest_html_report.html\" jest src/lo-regression.spec.js",
    "test:rtr-regression": "JEST_JUNIT_OUTPUT_NAME=\"rtr-regression.xml\" JEST_HTML_OUTPUT_NAME=\"jest_html_report.html\" jest src/rtr-regression.spec.js",
    "test:iws-nway-call": "JEST_JUNIT_OUTPUT_NAME=\"iws-nway-call.xml\" JEST_HTML_OUTPUT_NAME=\"jest_html_report.html\" jest src/iws-nway-call.spec.js",
    "test:outbound-pstn": "JEST_JUNIT_OUTPUT_NAME=\"outbound-pstn.xml\" JEST_HTML_OUTPUT_NAME=\"jest_html_report.html\" jest src/outbound-pstn.spec.js",
